pipeline {
  agent any
   stages {
    stage ('Clean') {
      steps {
        sh '''#!/bin/bash
        source test/bin/activate
        python setup2.py
        '''
     }
   }
 stage ('Deploy') {
  steps {
     sh '''#!/bin/bash
     source test/bin/activate
     python setup3.py
    '''
      }
    }
  }
}
